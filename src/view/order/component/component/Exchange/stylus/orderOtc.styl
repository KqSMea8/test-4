@import "variable.styl"
.order-otc
	.order-nav
		width 100%
		height 60px
		display flex
		background $order-title-bg
		align-items center
		li
			a
				display inline-block
				height 60px
				padding 0 24px
				line-height 60px
				font-size 18px
				color $order-font-content
				font-weight 500
				&.active
					background $colFFFF
					color $order-line-active
	.order-content-page
		height 78px
		display flex
		align-items center
		justify-content flex-end
		padding-right 24px