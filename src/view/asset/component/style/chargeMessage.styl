// @import "./base.styl"
.asset-message-wrap
    overflow hidden
    position fixed
    left 0
    top 60px
    width 100%
    height 0.44*100px
    background-color $asset-bg-none
    z-index 99
  .asset-message-bg
    background-color $asset-bg-active
    .asset-message
      position relative
      overflow hidden
      width 1200px
      height 0.44*100px
      line-height 0.44*100px
      margin 0 auto
      ul
        position absolute
        width 100%
        left 85px
        li
          float left
          width 50%
          height 0.44*100px
          font-size 0.13*100px
          color $asset-font-normal
          letter-spacing 0
          a
            color $asset-font-normal
          a:hover
            color $asset-font-negative
            text-decoration underline
            cursor pointer
      .asset-message-title
        float left
        padding-left 0.24*100px
        margin-right 0.1*100px
        background $asset-charge-new no-repeat 0 0.12*100px
        background-size 0.2*100px 0.2*100px